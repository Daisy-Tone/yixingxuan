.cartList{
  display: flex;
}
.block{
  width: 82%;
  height: 180rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  box-shadow: 2rpx 2rpx 5rpx 10rpx #f7f7f7;
}
.title-block{
  display: flex;
}
.image{
  width: 100rpx;
  height: 100rpx;
  margin-top: 5rpx;
  margin-left: 5rpx;
}
.title{
  margin-left: 10rpx;
}
.price{
  text-align: right;
  margin-right: 30rpx;
}
.icon-block{
  display: flex;
  width: 80rpx;
}
.icon{
  width: 45rpx;
  height: 45rpx;
  margin-left: 30rpx;
  margin-top: 90rpx;
}
.button{
  margin-top: 30rpx;
  margin-right: 20rpx;
  display: flex;
  flex-direction: row-reverse;
}
.buy{
  width:200rpx;
  height: 60rpx;
  margin-left: 20rpx;
  padding: 10rpx 10rpx;
  border-radius: 100rpx;
  background-color: #38aaf3;
  color: white;
  line-height: 60rpx;
  text-align: center;
}